# Fixes compdef not found error
autoload -Uz compinit
compinit -i

# Load plugins generated from ~/.dotfiles/zsh/plugins.txt
antibody bundle < ~/.dotfiles/zsh/plugins.txt > ~/.zsh_plugins.sh
source ~/.zsh_plugins.sh

# Load ZSH specific functions
source ~/.dotfiles/zsh/functions/functions.zsh

# Load Prompt customizations
source ~/.dotfiles/zsh/starship/custom.txt

# Load Aliases
source ~/.dotfiles/zsh/alias.zsh

# Load Environment Variables
source ~/.dotfiles/zsh/env.zsh
