# Edit files alias'
alias ezsh="vim ~/.dotfiles/zsh/.zshrc"
alias ealias="vim ~/.dotfiles/zsh/alias.zsh"
alias eplugins="vim ~/.dotfiles/zsh/plugins.txt"

# Source file alias'
alias szsh="source ~/.dotfiles/zsh/.zshrc"

# Cd Alias'
alias cdot="cd ~/.dotfiles"
alias cdata="cd ~/data"
alias cpg="cd ~/data/personal/pgblitz"

# Colorls Alias'
alias ls="colorls"
alias "ls -al"="colorls -al"
alias ll="colorls -al"

# Ssh Alias'
alias ssg="ssh golang"
